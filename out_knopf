    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 3 "counter";
    .port_info 2 /INPUT 1 "knopf";
    .port_info 3 /OUTPUT 2 "ampelfarbe";
v0x5c11c3f80340_0 .var "ampelfarbe", 1 0;
v0x5c11c3f80440_0 .net "clk", 0 0, v0x5c11c3f809d0_0;  1 drivers
v0x5c11c3f80500_0 .net "counter", 2 0, v0x5c11c3f80aa0_0;  1 drivers
v0x5c11c3f805c0_0 .net "knopf", 0 0, v0x5c11c3f80ba0_0;  1 drivers
