    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c11c3f80ba0_0, 0, 1;
T_4.0 ;
    %load/vec4 v0x5c11c3f80aa0_0;
    %pad/u 32;
    %pushi/vec4 7, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_4.1, 6;
    %wait E_0x5c11c3f60aa0;
    %jmp T_4.0;
T_4.1 ;
    %delay 1, 0;
