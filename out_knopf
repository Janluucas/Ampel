E_0x5c11c3f4a9b0 .event posedge, v0x5c11c3f80440_0;
S_0x5c11c3f658e0 .scope function.vec4.s2, "ampel_farbe" "ampel_farbe" 2 11, 2 11 0, S_0x5c11c3f65730;
