; Variable ampel_farbe is vec4 return value of scope S_0x5c11c3f658e0
v0x5c11c3f80260_0 .var "c", 2 0;
TD_main.ampel_Behavioral_inst.ampel_farbe ;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 0, 2;
    %inv;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 1, 2;
    %inv;
    %and;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 1, 2;
    %and;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 2, 3;
    %inv;
    %and;
    %or;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 0, 2;
    %inv;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 1, 2;
    %inv;
    %and;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 2, 3;
    %inv;
    %and;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 2, 3;
    %and;
    %or;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 0, 2;
    %inv;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 1, 2;
    %and;
    %load/vec4 v0x5c11c3f80260_0;
    %parti/s 1, 2, 3;
    %and;
    %or;
    %concat/vec4; draw_concat_vec4
    %ret/vec4 0, 0, 2;  Assign to ampel_farbe (store_vec4_to_lval)
    %end;
    .scope S_0x5c11c3f65730;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c11c3f807b0_0, 0, 1;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x5c11c3f80680_0, 0, 4;
    %end;
    .thread T_1;
    .scope S_0x5c11c3f65730;
T_2 ;
    %wait E_0x5c11c3f4a9b0;
    %load/vec4 v0x5c11c3f805c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5c11c3f807b0_0, 0;
T_2.0 ;
    %load/vec4 v0x5c11c3f80680_0;
    %cmpi/ne 8, 0, 4;
    %jmp/0xz  T_2.2, 4;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x5c11c3f80340_0, 0;
    %load/vec4 v0x5c11c3f80680_0;
    %cmpi/e 1, 0, 4;
    %jmp/0xz  T_2.4, 4;
    %pushi/vec4 8, 0, 4;
    %assign/vec4 v0x5c11c3f80680_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5c11c3f807b0_0, 0;
    %jmp T_2.5;
T_2.4 ;
    %load/vec4 v0x5c11c3f80680_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x5c11c3f80680_0, 0;
T_2.5 ;
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v0x5c11c3f80500_0;
    %store/vec4 v0x5c11c3f80260_0, 0, 3;
    %callf/vec4 TD_main.ampel_Behavioral_inst.ampel_farbe, S_0x5c11c3f658e0;
    %pushi/vec4 2, 0, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x5c11c3f807b0_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.6, 8;
    %pushi/vec4 7, 0, 4;
    %assign/vec4 v0x5c11c3f80680_0, 0;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x5c11c3f80340_0, 0;
    %jmp T_2.7;
T_2.6 ;
    %load/vec4 v0x5c11c3f80500_0;
    %store/vec4 v0x5c11c3f80260_0, 0, 3;
